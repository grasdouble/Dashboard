{
  "name": "@dashboard/backend",
  "version": "0.0.1",
  "main": "index.js",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir dist --copy-files",
    "prepublish": "npm run build",
    "start": "node dist/app.js",
    "test": "jest"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/app.js",
      "!**/node_modules/**",
      "!**/__snapshots__/**"
    ],
    "roots": [
      "src",
      "test"
    ],
    "testRegex": "&*\\.test\\.js$"
  },
  "dependencies": {
    "node-fetch": "^2.6.0",
    "server": "^1.0.21"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/runtime": "^7.7.6",
    "jest": "^24.9.0",
    "request-promises": "^1.1.0"
  }
}
